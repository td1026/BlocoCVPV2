# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET      /medicamento                   controllers.HomeController.findMedicamento
GET      /medicamento/:id               controllers.HomeController.getMedicamento(id: Integer)
POST     /medicamento                   controllers.HomeController.updadteMedicamento
PUT      /medicamento                   controllers.HomeController.createMedicamento
#DELETE   /medicamento                   controllers.HomeController.deleteMedicamento

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
